{
  "kind": "collectionType",
  "collectionName": "cities",
  "info": {
    "singularName": "city",
    "pluralName": "cities",
    "displayName": "City"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "CityName": {
      "type": "string"
    },
    "IsTop": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "districts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::district.district",
      "mappedBy": "city"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "city"
    }
  }
}
